; Name,alonpar,alatpar,wpath,interp_path,anim_path,min_nvect,decimate, crdecimate,excludecols


{ desig: 'NEPAC', alonpar: [195.,245,1.5], alatpar: [30., 60,1.5], wpath: "$VAP_WINDS", interp_path: "$VAP_ANIM/", anim_path: "$VAP_ANIM/nepac/daily", min_nvect: 4000,decimate: 0l, CRDecimate: [2,2], ExcludeCols: '0,38:42,75'  }
{ desig: 'NWPAC', alonpar: [110.,180,1.5], alatpar: [0.,  60,1.5], wpath: "$VAP_WINDS", interp_path: "$VAP_ANIM/", anim_path: "$VAP_ANIM/nwpac/daily", min_nvect:10000,decimate: 0l, CRDecimate: [2,2], ExcludeCols: '0,38:42,75'  }
{ desig: 'NWATL', alonpar: [270.,315,1.5], alatpar: [20,  45,1.5], wpath: "$VAP_WINDS", interp_path: "$VAP_ANIM/", anim_path: "$VAP_ANIM/nwatl/daily", min_nvect: 4400 ,decimate: 0l, CRDecimate: [2,2], ExcludeCols: '0,38:42,75' }
